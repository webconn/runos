{

"default": {
    "services": [
        "learning-switch",
        "rest-listener",
        "host-manager",
        "webui",
        "static-flow-pusher",
        "switch-stats",
        "stp",
        "arp-handler",
        "flow-manager",
        "rest-multipart",
        "rest-flowmod",
        "natswitch"
    ],

    "flow-manager" : {
        "interval" : 5
    },

    "rest-listener" : {
         "port" : 8000,
         "web-dir" : "./build/web"
    },

    "controller": {
         "nthreads": 4,
         "cbench": false,
         "pipeline": [
             "link-discovery",
             "host-manager",
             "arp-handler",
             "forwarding"
         ]
    },

    "loader": {
        "threads": 2
    },

    "link-discovery": {
        "poll-interval": 10,
        "pin-to-thread": 1
    },

    "switch-manager": {
        "pin-to-thread": 1
    },

    "switch-stats": {
	"poll-interval": 1,
	"pin-to-thread": 1
    },

    "natswitch": {
        "switch_dpid": 2,
        "local_subnet": "10.0.0.0/16",
        "address_pool": [
             "10.2.0.1",
             "10.2.0.2",
             "10.2.0.3"
        ]
    }
}

}
